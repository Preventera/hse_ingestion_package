{
  "workspace": "SafetyGraph-HSE",
  "project": "AgenticX5-Predictions",
  "environment": {
    "python_version": "3.10",
    "packages": [
      "pandas>=2.0",
      "scikit-learn>=1.3",
      "xgboost>=2.0",
      "lightgbm>=4.0",
      "mlflow>=2.8",
      "shap>=0.43",
      "plotly>=5.18"
    ]
  },
  "data_sources": [
    {
      "name": "safetygraph_postgres",
      "type": "postgresql",
      "host": "localhost",
      "port": 5434,
      "database": "safetygraph"
    },
    {
      "name": "safetygraph_neo4j",
      "type": "neo4j",
      "uri": "bolt://localhost:7688"
    }
  ],
  "experiments": [
    {
      "name": "risk_prediction_xgboost",
      "model_type": "XGBClassifier",
      "target": "risk_category",
      "features": [
        "year",
        "month",
        "total_cases",
        "days_lost",
        "severity_rate",
        "fatality_rate"
      ]
    },
    {
      "name": "severity_regression",
      "model_type": "XGBRegressor",
      "target": "severity_score",
      "features": [
        "year",
        "month",
        "total_cases",
        "days_lost",
        "severity_rate",
        "fatality_rate"
      ]
    },
    {
      "name": "incident_forecasting",
      "model_type": "Prophet",
      "target": "total_cases",
      "time_column": "year"
    }
  ]
}